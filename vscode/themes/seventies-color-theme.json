{
  "name": "GroovyCode",
  "type": "dark",
  "colors": {
    "editor.background": "#2D2320",
    "editor.foreground": "#ffd58e",
    "editor.lineHighlightBackground": "#352A26",
    "editor.selectionBackground": "#52b3b650",
    "editorCursor.foreground": "#D35D2B",

    "activityBar.background": "#2D2320",
    "activityBar.foreground": "#F5E6C9",
    "activityBarBadge.background": "#D35D2B",

    "sideBar.background": "#352A26",
    "sideBar.foreground": "#F5E6C9",

    "statusBar.background": "#D35D2B",
    "statusBar.foreground": "#F5E6C9",

    "titleBar.activeBackground": "#2D2320",
    "titleBar.activeForeground": "#F5E6C9"
  },
  "tokenColors": [
    {
      "name": "Keywords and Control",
      "scope": ["keyword", "storage", "control"],
      "settings": {
        "foreground": "#ff9b5a",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings and Text",
      "scope": ["string", "markup.quote"],
      "settings": {
        "foreground": "#6fcb9f"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": ["entity.name.function", "meta.method"],
      "settings": {
        "foreground": "#c847bd",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "support.variable"],
      "settings": {
        "foreground": "#ffd58e"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "support.constant"],
      "settings": {
        "foreground": "#52b3b6"
      }
    },
    {
      "name": "Classes and Types",
      "scope": ["entity.name.type", "support.class"],
      "settings": {
        "foreground": "#D35D2B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#ff6e6e",
        "fontStyle": "italic bold"
      }
    }
  ]
}